                    Re_Init_Keycodes()
                    task.wait(0.015)
                    k = CHA_keycode;
                    if Dependencies.GetCurrentCharacter() and Dependencies.GetCurrentCharacter():FindFirstChild("Crucifix") and Dependencies.GetCurrentCharacter():FindFirstChild("Crucifix"):FindFirstChild("Handle")   then
